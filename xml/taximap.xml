<Definition>
  <Import url="[config.mod.template]button/iconButton.xml"/>
  <Import url="[config.mod.template]button/simpleButton.xml"/>
  <Constants>
    <Constant name="maps_uri" value="[config.gfx.path.maps]"/>
    <Constant name="assets" value="[config.ui.skin]assets.swf|"/>
    <Constant name="bitmap" value="[config.ui.skin]bitmap/"/>
    <Constant name="css" value="[config.ui.skin]css/"/>
  </Constants>
  <Container name="ctr_main">
    <Anchors>
      <Anchor>
        <AbsDimension x="200" y="100"/>
      </Anchor>
    </Anchors>
    <Texture name="tx_background">
      <autoGrid>true</autoGrid>
      <Size>
        <AbsDimension x="880" y="700"/>
      </Size>
      <uri>[local.assets]tx_generalBackgroundWithTitle</uri>
    </Texture>
    <Texture name="tx_validButton">
      <Size>
        <AbsDimension x="280" y="36"/>
      </Size>
      <uri>[local.assets]tx_buttonBackground</uri>
      <Anchors>
        <Anchor>
          <AbsDimension x="300" y="693"/>
        </Anchor>
      </Anchors>
    </Texture>
    <MapViewer name="mv_teleportMap">
      <roundCornerRadius>15</roundCornerRadius>
      <Size>
        <AbsDimension x="840" y="600"/>
      </Size>
      <Anchors>
        <Anchor>
          <AbsDimension x="20" y="55"/>
        </Anchor>
      </Anchors>
    </MapViewer>
    <Texture name="tx_mapBorder">
      <autoGrid>true</autoGrid>
      <Size>
        <AbsDimension x="840" y="610"/>
      </Size>
      <uri>[local.assets]tx_blockBorder</uri>
      <Anchors>
        <Anchor>
          <AbsDimension x="19" y="50"/>
        </Anchor>
      </Anchors>
    </Texture>
    <iconButton name="btn_close">
      <Size>
        <AbsDimension x="57" y="36"/>
      </Size>
      <uri>[local.assets]btn_cornerCross</uri>
      <Anchors>
        <Anchor relativeTo="tx_background" point="TOPRIGHT" relativePoint="TOPRIGHT"/>
      </Anchors>
    </iconButton>
    <simpleButton name="btn_valid" label="[ui.common.validation]">
      <Size>
        <AbsDimension x="104" y="30"/>
      </Size>
      <Anchors>
        <Anchor relativeTo="tx_validButton">
          <AbsDimension x="88" y="1"/>
        </Anchor>
      </Anchors>
    </simpleButton>
    <Texture name="tx_kama">
      <Size>
        <AbsDimension x="38" y="38"/>
      </Size>
      <uri>[local.assets]tx_kama</uri>
      <Anchors>
        <Anchor>
          <AbsDimension x="827" y="651"/>
        </Anchor>
      </Anchors>
    </Texture>
    <Label name="lbl_positionName">
      <css>[local.css]title.css</css>
      <Size>
        <AbsDimension x="700" y="37"/>
      </Size>
      <text>Character position</text>
      <Anchors>
        <Anchor>
          <AbsDimension x="56" y="16"/>
        </Anchor>
      </Anchors>
    </Label>
    <Texture name="tx_spawnPoint">
      <Size>
        <AbsDimension x="21" y="30"/>
      </Size>
      <uri>[local.assets]zaap_tx_respawn</uri>
      <Anchors>
        <Anchor>
          <AbsDimension x="25" y="18"/>
        </Anchor>
      </Anchors>
    </Texture>
    <Label name="lbl_kamas">
      <css>[local.css]normal.css</css>
      <cssClass>right</cssClass>
      <Size>
        <AbsDimension x="120" y="30"/>
      </Size>
      <selectable>false</selectable>
      <text>2,000,000,000</text>
      <Anchors>
        <Anchor>
          <AbsDimension x="708" y="657"/>
        </Anchor>
      </Anchors>
    </Label>
    <Label name="lbl_destinationName">
      <css>[local.css]normal.css</css>
      <cssClass>p</cssClass>
      <Size>
        <AbsDimension x="550" y="20"/>
      </Size>
      <text>Default destination</text>
      <Anchors>
        <Anchor>
          <AbsDimension x="32" y="658"/>
        </Anchor>
      </Anchors>
    </Label>
    <Label name="lbl_destinationCost">
      <css>[local.css]normal.css</css>
      <cssClass>right</cssClass>
      <Size>
        <AbsDimension x="60" y="21"/>
      </Size>
      <text>10,000</text>
      <Anchors>
        <Anchor>
          <AbsDimension x="585" y="657"/>
        </Anchor>
      </Anchors>
    </Label>
    <Texture name="tx_rightArrow">
      <uri>[local.assets]cursorRight</uri>
      <Size>
        <AbsDimension x="30" y="20"/>
      </Size>
      <Anchors>
        <Anchor>
          <AbsDimension x="711" y="671"/>
        </Anchor>
      </Anchors>
    </Texture>
    <Texture name="tx_kamasCost">
      <Size>
        <AbsDimension x="38" y="38"/>
      </Size>
      <uri>[local.assets]tx_kama</uri>
      <Anchors>
        <Anchor>
          <AbsDimension x="644" y="651"/>
        </Anchor>
      </Anchors>
    </Texture>
  </Container>
</Definition>